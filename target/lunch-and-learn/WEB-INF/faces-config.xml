<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <navigation-rule>
  <from-view-id>/pages/employee/list.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>search</from-outcome>
   <to-view-id>/pages/employee/list.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>load</from-outcome>
   <to-view-id>/pages/employee/form.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>merge</from-outcome>
   <to-view-id>/pages/employee/list.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>prepare</from-outcome>
   <to-view-id>/pages/employee/form.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/employee/form.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>create</from-outcome>
   <to-view-id>/pages/employee/list.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>merge</from-outcome>
   <to-view-id>/pages/employee/list.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
